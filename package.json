{
  "name": "commerce-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://fran.github.io/react-shop",
  "dependencies": {
    "axios": "^0.21.1",
    "axios-oauth-client": "^1.1.1",
    "axios-token-interceptor": "^0.1.0",
    "client-oauth2": "^4.2.4",
    "follow-redirects": "^1.7.0",
    "gh-pages": "^2.0.1",
    "prop-types": "latest",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-oauth-flow": "^1.2.0",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "redux-implicit-oauth2": "^1.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-app-rewired build && yarn run build:dist",
    "build:dist": "cd build && cp static/js/*.js ../dist/shop.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-app-rewired": "^2.1.3",
    "react-redux": "^7.1.0"
  }
}
